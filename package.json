{
  "name": "canvasom",
  "version": "0.16.1",
  "description": "Canvas Object Model.",
  "module": "./dist/canvasom.min.js",
  "main": "./dist/canvasom.umd.min.js",
  "types": "./index.d.ts",
  "author": "3h",
  "license": "MIT",
  "scripts": {
    "prebuild": "tsc",
    "build": "rollup -c",
    "postbuild": "terser dist/canvasom.js --config-file terser.json --module -o dist/canvasom.min.js && terser dist/canvasom.umd.js --config-file terser.json -o dist/canvasom.umd.min.js && node stat",
    "docs": "dts2md \"**\" \"!index.d.ts\" -i ./types -o ./docs -l -I API.md"
  },
  "repository": "huang2002/canvasom",
  "keywords": [
    "canvas",
    "object",
    "model",
    "com",
    "3h"
  ],
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "3h-event": "^0.2.0",
    "3h-test": "^0.6.0",
    "3h-utils": "^0.1.0",
    "dts2md": "^0.4.3",
    "rollup": "^2.26.0",
    "terser": "^5.2.0",
    "typescript": "^4.0.0"
  }
}
